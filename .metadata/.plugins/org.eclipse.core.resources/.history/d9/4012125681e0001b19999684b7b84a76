package bookshelf_LYJ;

import java.util.ArrayList;

public class Shelf {
	protected ArrayList<String> shelf; //배열 정의
	
	public Shelf() {
		shelf = new ArrayList<String>(); //배열 생성
	}
	
	public ArrayList<String> getShelf(){ //배열 반환
		return shelf;
	}
	
	public int getCount() {
		return shelf.size();
	}
}
