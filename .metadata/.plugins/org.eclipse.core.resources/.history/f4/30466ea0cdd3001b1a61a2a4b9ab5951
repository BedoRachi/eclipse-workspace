package chapter05;

public class If_Example06 {

	public static void main(String[] args) {
		int score = 100;
		char grade = 'X';
		if(score > 100) {
			System.out.println("잘못된 값을 입력하였습니다.0~100의 값을 넣으시요");
		}
		else if(score < 0){
			System.out.println("잘못된 값을 입력하였습니다.0~100의 값을 넣으시요");
		}
		else if(score >= 90){
			grade = 'A';
		}
		else if(score >= 80){
			grade = 'B';
		}
		else if(score >= 70){
			grade = 'C';
		}
		else if(score >= 60){
			grade = 'D';
		}
		else {
			grade = 'F';
		}
		System.out.println("점수는 " + grade + " 입니다.");
	}
}
