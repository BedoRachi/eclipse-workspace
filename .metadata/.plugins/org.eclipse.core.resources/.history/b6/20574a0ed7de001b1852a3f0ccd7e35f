package instanceofex;

class Person{}
class Student extends Person{}
class Resercher extends Person{}
class Professor extends Resercher{}

public class InstanceOfEx {
	
	static void print(Person p) {
		if(p instanceof Person) {
			System.out.println("Person");
		} else if(p instanceof Student) {
			System.out.println("Student");
		} else if(p instanceof Resercher) {
			System.out.println("Resercher");
		} else if(p instanceof Professor) {
			System.out.println("Professor");
		}
	}

	public static void main(String[] args) {
		System.out.print("new Student() -> \t"); print(new Student());
		System.out.print("new Student() -> \t"); print(new Resercher());
		System.out.print("new Student() -> \t"); print(new Professor());
	}

}
