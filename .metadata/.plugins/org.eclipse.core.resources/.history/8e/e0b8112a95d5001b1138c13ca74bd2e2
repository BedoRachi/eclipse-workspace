package chapter05;

import java.util.Scanner;

public class Book_Test {
	
	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
		
		System.out.println("책의 이름을 입력하시오");
		
		Book b1 = new Book(scan.nextLine());
		Book b2 = new Book("인자가 2개인", "생성자를 만듬");
		
		System.out.println("책이름: " + b1.title + " 저자: " + b1.name);
		System.out.println("책이름: " + b2.title + " 저자: " + b2.name);
		
	}

}
