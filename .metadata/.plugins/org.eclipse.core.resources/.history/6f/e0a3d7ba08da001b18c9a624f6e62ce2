package array;

public class ArrayTest {
	
	public static void main(String[] args) {
		int[] num = new int[10];
		int total=0;
		int temp = 0;
		
		for(int i=1; i < 11; i++ ) {
			num[temp] = i;
			total += num[temp];
			temp++;

		}
		for(int j=0; j < 10; j++ ) {
			System.out.println(num[j] + " " + total);
		}
		
		System.out.println(num.length);
	}

}

